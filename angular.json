{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "aysasoft": {
      "projectType": "library",
      "root": "projects/aysasoft",
      "sourceRoot": "projects/aysasoft/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/aysasoft/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/aysasoft/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/aysasoft/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    },
    "@aysasoft/language": {
      "projectType": "library",
      "root": "projects/aysasoft/language",
      "sourceRoot": "projects/aysasoft/language/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/aysasoft/language/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/aysasoft/language/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/aysasoft/language/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/aysasoft/language/src/test.ts",
            "tsConfig": "projects/aysasoft/language/tsconfig.spec.json",
            "karmaConfig": "projects/aysasoft/language/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "aysasoft"
}
